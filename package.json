{
  "name": "react-scroll-section-monorepo",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "devDependencies": {
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "start-server-and-test": "^1.14.0"
  },
  "scripts": {
    "build": "yarn workspace react-scroll-section build",
